    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "no arguments",
            "program": "${workspaceFolder}/index.js",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "help",
            "program": "${workspaceFolder}/index.js",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "args": [ "help" ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "photons",
            "program": "${workspaceFolder}/index.js",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "args": [ "photons" ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "devices",
            "program": "${workspaceFolder}/index.js",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "args": [ "devices" ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "on office",
            "program": "${workspaceFolder}/index.js",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "args": [ "on", "office" ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "off office",
            "program": "${workspaceFolder}/index.js",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "args": [ "off", "office" ]
        }
    ]
}